

<form id="project-form">
  <div class="container projForm">
   
        <div class="col-md-6 col-md-offset-3">
            <h3>Project:</h3>
            <input class="form-control" type="text" placeholder="Project">
        </div>
        
        <div class="col-md-6 col-md-offset-3">
            <h3>Client:</h3>
            <input class="form-control" type="text" placeholder="Client Name">
        </div>
        <div class="col-md-6 col-md-offset-3">
            <h3>Project Number:</h3>
            <input class="form-control" type="text" placeholder="Project Number">
        </div>

        <div class="col-md-6 col-md-offset-3">
            <h3>Start Date:</h3>
            <input class="form-control" type="text" placeholder="Ex: 08/17/20">
        </div>

        <div class="col-md-6 col-md-offset-3">
            <h3>End Date:</h3>
            <input class="form-control" type="text" placeholder="Ex: 10/22/20">
        </div>

        <div class="col-md-6 col-md-offset-3">
            <div class="submitBtn">
                <input class="btn btn-primary" type="submit" value="Submit">
            </div>
        </div>
  </div>
</form>