<form id="project-form" class="card">
  <div class="container projForm">
   
        <div class="form-group">
            <h3 class="display-6 text-center">Project:</h3>
            <input class="form-control" id="proj-name" type="text" placeholder="Project">
        </div>
          
        <div class="form-group">
          <h3 class="display-6 text-center" for="exampleFormControlSelect1">Assign Employee:</h3>
           <select class="form-control employee-names" id="exampleFormControlSelect1">

            {{#each employees}}

              <option value="{{ id }}">{{ name }}</option>

            {{/each}}

          </select>
       </div>

        <div class="form-group">
            <h3 class="display-6 text-center">Start Date:</h3>
            <input class="form-control" id="start-date" type="text" placeholder="Ex: 08/17/20">
        </div>

        <div class="form-group">
            <h3 class="display-6 text-center">End Date:</h3>
            <input class="form-control" id="end-date" type="text" placeholder="Ex: 10/22/20">
        </div>
        
        <div class="form-group">
            <div class="submitBtn">
                <input class="btn btn-primary" type="submit" value="Submit">
            </div>
        </div>
  </div>
</form>
    
