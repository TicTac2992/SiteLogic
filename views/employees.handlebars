<form id="employee-form">
  <div class="container form addForm">
   
        <div class="col-md-6 col-md-offset-3">
            <h3>Name:</h3>
            <input class="form-control" id="em-name" type="text" placeholder="Ex: John Smith">
        </div>
        
        <div class="col-md-6 col-md-offset-3">
            <h3>Title:</h3>
            <input class="form-control" id="em-title" type="text" placeholder="Ex: Operations Manager">
        </div>
        <div class="col-md-6 col-md-offset-3">
            <h3>Email:</h3>
            <input class="form-control" id="em-email" type="text" placeholder="Ex: jsmith@example.com">
        </div>

        <div class="col-md-6 col-md-offset-3">
            <h3>Phone:</h3>
            <input class="form-control" id="em-phone" type="text" placeholder="Ex: 6159872456">
        </div>


        <div class="col-md-6 col-md-offset-3">
            <div class="submitBtn">
                <input class="btn btn-primary" type="submit" value="Submit">
            </div>
        </div>
  </div>
</form>